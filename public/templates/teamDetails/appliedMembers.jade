.row.dashContent
  div.row.dashContentTeam(ng-repeat='member in team.appliedMembers')
    a(href='/userprofile/{{member._id.profile.slug}}')
      .col-md-1.col-sm-1.col-xs-2.teamPhoto
        img(ng-src='{{member._id.profile.picture}}')
      .col-md-9.col-sm-8.col-xs-5
        h4 {{member._id.profile.name}}
    .col-md-2.col-sm-3.col-xs-5
      .col-md-1.col-sm-1.col-xs-1.approval.approvalUserProfile
        img.approveMemberIcon(ng-src='images/approve-member.svg', ng-click='approveMember(member._id.profile.slug)')
      .col-md-1.col-sm-1.col-xs-1.disproval.disprovalUserProfile
        img.disproveMemberIcon(ng-src='images/disprove-member.svg', ng-click='disproveMember(member._id.profile.slug)')
      br
  div.noMembersYet.col-md-12(ng-hide='{{team.appliedMembers[0]}}') 
    h4 No members have applied to your team yet.
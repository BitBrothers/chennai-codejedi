.row.dashContent
  div.row.dashContentTeam(ng-repeat='member in team.appliedMembers')
    a(href='/userprofile/{{member._id.profile.slug}}')
      .col-md-1.col-xs-2.teamPhoto
        img(ng-src='{{member._id.profile.picture}}')
      .col-md-9.col-xs-8
        h4 {{member._id.profile.name}}
    .col-md-1.col-xs-1.approval
      img.approveMemberIcon(ng-src='images/approve-member.svg', ng-click='approveMember(member._id.profile.slug)')
    .col-md-1.col-xs-1.disproval
      img.disproveMemberIcon(ng-src='images/disprove-member.svg', ng-click='approveMember(member._id.profile.slug)')
      br
  div.col-md-12(ng-hide='{{team.appliedMembers}}') 
    h4 No members have applied to your team yet.
.row.dashContent
  div.row.dashContentTeam(ng-repeat='applied in user.events[0].appliedTeams')
    a(href='/team/{{applied._id.slug}}')
      .col-md-1.col-xs-2.teamPhoto
        img(src='images/lannister.png')
      .col-md-10.col-xs-8
        h4 {{applied._id.name}}
      .col-md-1.col-xs-2
        h4 x
      br
  div.col-md-12(ng-hide='user.events[0].appliedTeams') 
    h4 You have not applied to any team yet.  
// dashContent ENDS